{
  "compilerOptions": {
    "target": "ES6",
    "strict": false,
    "module": "commonjs",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@data/*": ["src/data/*"],
      "@global/*": ["src/global/*"],
      "@base/*": ["src/base/*"],
      "@support/*": ["src/support/*"],
      "@utils/*": ["src/utils/*"],
    }
  }
}
